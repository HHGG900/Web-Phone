			document.getElementById("bmi").innerHTML = "<font onclick='BMI()' id='bmi' color='#FF0000' size='5'>"+
				"BMI:" + bmi+ "     </font >"
		}

	function get_value(a){

	var arr2 = [];
	$(a).each(function () {
	arr2.push($(this).val());
	});
	return arr2.toString();

	}
		function unload() {


			var location = get_value("input[name='location']:checked");
