@using Web_Phone.Models;
@{
	List<Exception_notification> exception_notifications = ViewBag.exception_notifications;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
	<table class="table">
		<thead>
			<tr>
				<th>人員姓名</th>
				<th>事件描述</th>
				<th>異常類別</th>
				<th>十大指標</th>
				<th>送出時間</th>
			</tr>
		</thead>
		<tbody>
			@foreach(Exception_notification exception_Notification in exception_notifications)
			{
			<tr>
				<th>@exception_Notification.working_name</th>
				<th>@exception_Notification.event_description</th>
				<th>@exception_Notification.exception_class</th>
				<th>@exception_Notification.ten_index</th>
				<th>@exception_Notification.tim</th>
			</tr>
				
			}
		</tbody>
	</table>
	<input type="button" class="btn btn-primary" value="返回" onclick="location.href='/Home/Abnormal_events_reactions_Home_attendant'" />
</body>
</html>
